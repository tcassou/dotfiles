server {
        listen          80;
	root	/usr/local/var/www/adminer;
	index index.php;
        server_name     adminer.local www.adminer.local;

	error_log /usr/local/etc/nginx/logs/adminer.error.log;
	access_log  /usr/local/etc/nginx/logs/adminer.access.log main;
        include   /usr/local/etc/nginx/conf.d/php-fpm;
}

